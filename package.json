{
  "name": "lifeapp",
  "version": "2.3.0",
  "clientVersion": "2.2.10",
  "description": "Level up with LifeApp server repository",
  "main": "lifeapp",
  "scripts": {
    "client:build:heroku": "tar -zxvf client-packages/client-package-heroku-$npm_package_clientVersion.tar.gz",
    "client:build:local": "tar -zxvf client-packages/client-package-local-$npm_package_clientVersion.tar.gz",
    "dev": "NODE_ENV=development nodemon",
    "dev:debug:agenda": "DEBUG=\"agenda:*\" NODE_ENV=development nodemon",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "init:db:local": "mongo --eval \"var DB_NAME='lifeapp'\" scripts/initDatabase.js",
    "init:db:remote": "mongo --eval \"var DB_NAME='mo1563_lifeapp'\" --port 27017 -u mo1563_lifeapp -p Gitara15 --host 85.194.240.29 mo1563_lifeapp scripts/initDatabase.js",
    "start": "yarn run client:build:heroku && DB=remote ts-node src",
    "start:local": "yarn run client:build:local && ts-node src",
    "add-tag": "git tag -a v$npm_package_version -m \"server version $npm_package_version\"",
    "test": "DEBUG=jest-mongodb:* jest --coverage --detectOpenHandles"
  },
  "keywords": [
    "express",
    "graphql",
    "web-push",
    "agenda",
    "mongoose"
  ],
  "author": "G3F4.XYZ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g3f4-xyz/lifeapp-server.git"
  },
  "bugs": {
    "url": "https://github.com/g3f4-xyz/lifeapp-server/issues"
  },
  "homepage": "https://github.com/g3f4-xyz/lifeapp-server#readme",
  "engines": {
    "node": "12.*.*"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "1.1.1",
    "@types/graphql": "14.5.0",
    "@types/jest": "24.0.11",
    "@types/passport-google-oauth20": "2.0.2",
    "jest": "24.5.0",
    "nodemon": "1.17.4",
    "ts-jest": "24.0.0",
    "tslint": "5.12.1"
  },
  "dependencies": {
    "@types/agenda": "2.0.5",
    "@types/body-parser": "1.17.1",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.1",
    "@types/express-session": "1.15.14",
    "@types/moment-timezone": "0.5.12",
    "@types/mongodb": "3.3.3",
    "@types/mongoose": "5.5.18",
    "@types/morgan": "1.7.37",
    "@types/node": "12.7.8",
    "@types/passport": "1.0.1",
    "@types/useragent": "2.1.1",
    "@types/web-push": "3.3.0",
    "agenda": "2.1.0",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "express-session": "1.15.6",
