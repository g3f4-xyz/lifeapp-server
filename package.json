{
  "name": "lifeapp",
  "version": "1.1.0",
  "description": "Level up with LifeApp server repository",
  "main": "lifeapp",
  "scripts": {
    "client:build:heroku": "tar -zxvf client-packages/client-package-heroku-1.1.3.tar.gz",
    "client:build:local": "tar -zxvf client-packages/client-package-local-1.1.3.tar.gz",
    "dev": "NODE_ENV=development nodemon src --watch ./src",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "init:db:local": "mongo --eval \"var DB_NAME='lifeapp'\" scripts/initDatabase.js",
    "init:db:remote": "mongo --eval \"var DB_NAME='mo1563_lifeapp'\" --port 27017 -u mo1563_lifeapp -p Gitara15 --host 85.194.240.29 mo1563_lifeapp scripts/initDatabase.js",
    "start": "npm run client:build:heroku && DB=remote node src",
    "start:local": "npm run client:build:local && DB=remote node src",
    "add-tag": "git tag -a v$npm_package_version -m \"server version $npm_package_version\"",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "keywords": [
    "express",
    "graphql",
    "web-push",
    "agenda",
    "mongoose"
  ],
  "author": "G3F4.XYZ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g3f4-xyz/lifeapp-server.git"
  },
  "bugs": {
    "url": "https://github.com/g3f4-xyz/lifeapp-server/issues"
  },
  "homepage": "https://github.com/g3f4-xyz/lifeapp-server#readme",
  "engines": {
    "node": "10.*.*"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "nodemon": "1.17.4"
  },
  "dependencies": {
    "agenda": "1.0.3",
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-graphql": "0.6.12",
    "express-session": "1.15.6",
    "graphql": "0.13.2",
    "graphql-relay": "0.5.5",
    "moment": "2.22.2",
    "mongoose": "5.1.1",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "web-push": "3.3.1"
  }
}
